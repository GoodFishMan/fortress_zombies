"VertexlitGeneric"
{
	"$baseTexture" "models/workshop_partner/player/items/heavy/heavy_hockeyhair/heavy_hockeyhair"
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .01
	"$detailblendmode" 6

	"$yellow" "0"
	"$one" "1"

	"$phong" "1"
	"$phongexponent" "5"
	"$phongboost" ".1"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "1"
	
	"$rimlight" "1"										
	"$rimlightexponent" "25"
	"$rimlightboost" "1.5"

	"$blendtintbybasealpha" "1"
	"$blendtintcoloroverbase" "0"

	"$colortint_base" "{45 45 36}"
	"$colortint_tmp" "[0 0 0]" 


	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"ItemTintColor"
		{
			"resultVar" "$colortint_tmp"
		}
		"SelectFirstIfNonZero"
		{
			"srcVar1"	"$colortint_tmp"
			"srcVar2"	"$colortint_base"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}