"VertexLitGeneric"
{
	"$bumpmap" "models\player\mercenary/mercenary_normal"
	
	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" ".0"
	"$detailblendmode" "6"
	
	"$yellow" "0"
	
	"$phong" "1"
	"$phongexponenttexture" "models\player\mercenary/mercenary_exponent"
	"$phongboost" "1"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "0"

	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "1"										// Boost for ambient cube component of rim lighting
	"$rimmask" "1"  
	
	"$ambientocclusion" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	
	"$envmap" "models/items/berserk_glow"
	"$envmaptint" "[0 0 0]"
	"$envmaptint_rage" "[1 0 0]"
	"$envmaptint_orig" "[0 0 0]"
	"$envmapfresnel" "1"	
	
	"$rage" "1"
	"$rage_compare" ".5"
	"$phongfresnelranges_rage"	"[0 1 10]"
	"$phongfresnelranges_orig"	"[.3 1 8]"
	
	"Proxies"
	{
		"spy_invis"
		{
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
		"RipAndTear"
		{
			"resultVar" "$rage"
		}
		"LessOrEqual"
		{
			"lessEqualVar"	"$phongfresnelranges"
			"greaterVar"	"$phongfresnelranges_orig"
			"srcVar1"		"$rage"
			"srcVar2"		"$rage_compare"
			"resultVar"		"$phongfresnelranges"
		}
		"LessOrEqual"
		{
			"lessEqualVar"	"$envmaptint"
			"greaterVar"	"$envmaptint_orig"
			"srcVar1"		"$rage"
			"srcVar2"		"$rage_compare"
			"resultVar"		"$envmaptint"
		}
		"LessOrEqual"
		{
			"lessEqualVar"	"$phongfresnelranges"
			"greaterVar"	"$phongfresnelranges_rage"
			"srcVar1"		"$rage_compare"
			"srcVar2"		"$rage"
			"resultVar"		"$phongfresnelranges"
		}
		"LessOrEqual"
		{
			"lessEqualVar"	"$envmaptint"
			"greaterVar"	"$envmaptint_rage"
			"srcVar1"		"$rage_compare"
			"srcVar2"		"$rage"
			"resultVar"		"$envmaptint"
		}
	}
}
