"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items\sa_plain.vtf"
	
	"$phong" "1"
	"$phongexponent" "12"
	"$phongboost" "1.2"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
//	"$lightwarptexture" "models\player\pyro\pyro_lightwarp"
//	"$lightwarptexture" "models\weapons\c_items\c_ambassador_lightwarp"
	"$phongfresnelranges"	"[1.5 3 1]"
	"$basemapalphaphongmask" "1"
	
	"$rimlight" "1"
	"$rimlightexponent" "20"	
	"$rimlightboost" "0.125"

	
	"360?$color2" "[ 0.9 0.8 0.8 ]"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	
	
		"WeaponSkin" {
		}
	}
}