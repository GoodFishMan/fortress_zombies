"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\c_items\Texan Rancher/Stampede_Destroyer"
	$BASEMAPALPHAPHONGMASK 1

	"$normalmapalphaenvmapmask" 1
	"$envmap" "env_cubemap"
	"$envmaptint" "[.18 .18 .18]"

	"$phong" "1"
	"$phongexponent" "20"
	"$phongboost" "8"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"

	"$rimlight" "1"
	"$rimlightexponent" "10"	
	"$rimlightboost" "1.3"



	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"weapon_invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}

