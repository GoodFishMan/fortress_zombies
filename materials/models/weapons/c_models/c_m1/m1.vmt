"Vertexlitgeneric"
{
	$basetexture "models\weapons\c_models\c_m1\m1_d"
	$bumpmap "models\weapons\c_models\c_m1\m1_n"

	$phong 1
	$phongboost 0.08
	$phongfresnelranges "[5 10 10]"
	$phongtint "[.95 .84 1]"
	$phongexponenttexture "models\weapons\c_models\c_m1\m1_s"
	$normalmapalphaphongmask 1

	$rimlight 1
	$rimlightexponent 22
	$rimlightboost 0.08
	$rimmask 1

	$envmap env_cubemap
	$envmaptint "[.005 .005 .005]"
	$basetexturealphaenvmapmask   1

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}