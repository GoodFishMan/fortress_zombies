"VertexlitGeneric"
{
	"$baseTexture" "models\weapons\v_levergun\c_levergun_lenses"

	"$basemapalphaphongmask" "1"
	"$basealphaenvmapmask" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.06 .05 .04]"	

	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "5"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"

	"$tempvar" "0"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
