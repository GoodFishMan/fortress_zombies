"VertexLitGeneric"
{
	"$basetexture" "models\weapons\v_umbrella\v_umbrella_red"
	"$bumpmap" "models\weapons\v_umbrella\v_umbrella_n"
//	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
//	"$detailscale" "5"
//	"$detailblendfactor" .01
//	"$detailblendmode" 6


	"$yellow" "0"
	"$one" "1"

	"$phong" "1"
	"$phongexponent" "10"
	"$phongboost" "1.6"	
	"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "0"

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "3"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "0.5"										// Boost for ambient cube component of rim lighting

	"$glowcolor" "1"
	
	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}
		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}