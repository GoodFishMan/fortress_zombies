VertexLitGeneric
{
	$basetexture "models\weapons\v_sniper_revolver\v_sniper_revolver"
	$bumpmap "models\weapons\v_sniper_revolver\v_sniper_revolver_n"

	"$phong" "1"
	"$phongexponent" "15"
	"$phongboost" "5"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.3 .3 3]"

	"$tempvar" "0"

	"$basephongexponent" "25"
	"$basephongboost" "5"

	"$commadd_phongexponent" "30"
	"$commadd_phongboost" 	"22.5"
	"$commweapon"		"0"

	"$rimlight" "1"
	"$rimlightexponent" "3"	
	"$rimlightboost" ".25"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}