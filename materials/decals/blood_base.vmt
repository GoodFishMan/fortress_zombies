"vertexlitgeneric"
{
	"$alphatest" 1
	"$decal" 1
	"$decalscale" 0.25
	
	$alphatestreference 0.7
	
	"$angle" "0.0"
    "$translate" "[0.0 0.0]"
    "$center" "[-0.5 -0.5]"

   	"Proxies"
    {
        "EntityRandom"
        {
            "scale" "360"
            "resultVar" "$angle"
        }
        "TextureTransform"
        {
            "translateVar" "$translate"
            "rotateVar" "$angle"
            "centerVar" "$center"
            "resultVar" "$basetexturetransform"
        }
	}
}
