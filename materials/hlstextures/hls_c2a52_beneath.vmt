"LightmappedGeneric"
{
        "$abovewater" 1	
	"$basetexture" "hlstextures/hls_c2a52"
	"$scale" "[.5 .5]"
	"$translucent" "1"

	"$additive" 1

        	"$refracttexture" "_rt_WaterRefraction"
	"$refractamount" "0"
        
	"%compilewater" 1
	"$surfaceprop" "water"
	"$bumpframe" "0"
	"$fogenable" 1
	"$fogcolor" "{ 1 19 47 }"
	"$fogstart" 0.00
	"$fogend" 600.00

	"$texoffset" "[0 0]"
	"$texscale" "1"
	

	"Proxies"
	{

		"TextureScroll"
		{
			"texturescrollvar" "$texoffset"
			"texturescrollrate" .20
			"texturescrollangle" 90.00
		}

		"TextureTransform"
		{
			"translateVar" "$texoffset"
			"scaleVar" "$texscale"
			"resultVar" "$basetexturetransform"
		}
	}
}