"LightmappedGeneric"
{
	"$abovewater" 0	
	"$basetexture" "hlstextures/hls_c2a1aw00"
	"$scale" "[.5 .5]"
	"$translucent" "1"
	"$alpha" "0.99"
	"$refracttexture" "_rt_WaterRefraction"
	"$refractamount" "0"
        
	"%compilewater" 1
	"$surfaceprop" "water"
	"$bottommaterial" "hlstextures/hls_c2a1aw00_beneath"
	"$fogenable" 1
	"$fogcolor" "{ 35 143 187 }"
	"$fogstart" 0.00
	"$fogend" 1000.00

	"$texoffset" "[0 0]"
	"$texscale" "1"
	

	"Proxies"
	{

		"TextureScroll"
		{
			"texturescrollvar" "$texoffset"
			"texturescrollrate" .20
			"texturescrollangle" 90.00
		}

		"TextureTransform"
		{
			"translateVar" "$texoffset"
			"scaleVar" "$texscale"
			"resultVar" "$basetexturetransform"
		}
	}
}